{
  "expo": {
    "name": "Praxis",
    "slug": "Praxis",
    "version": "0.4.0",
    "orientation": "portrait",
    "icon": "./assets/images/praxis-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#25292e",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#25292e"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "backgroundColor": "#25292e",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/praxis-icon.png",
        "backgroundColor": "#25292e"
      },
      "package": "com.jlucasgaldino.Praxis",
      "blockedPermissions": ["android.permission.READ_EXTERNAL_STORAGE", "android.permission.SYSTEM_ALERT_WINDOW"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "acca3b0d-5573-46fa-858a-88bae130eea4"
      }
    }
  }
}
